<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="bean.UserPlugin" table="UserPlugin">
        <id name="id" column="id" type="int"/>
        <property name="chatId" column="chat_id" type="long"/>
        <property name="pluginName" column="plugin_name" type="string"/>
    </class>
</hibernate-mapping>