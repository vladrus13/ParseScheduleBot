<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="bean.UserSubjects" table="UserSubject">
        <id name="chatId" column="chat_id" type="long"/>
        <property name="added" column="added" type="string"/>
        <property name="removed" column="removed" type="string"/>
    </class>
</hibernate-mapping>